    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0",
    "babel-plugin-angularjs-annotate": "^0.9.0",
    "base64url": "^2.0.0",
    "cache-loader": "^1.2.2",
    "cookie": "^0.3.1",
    "copy-webpack-plugin": "^4.5.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
